{
	"name": "autopep8",
	"displayName": "autopep8",
	"version": "0.0.1",
	"private": false,
	"description": "%extension.description%",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./dist/extension.js",
	"scripts": {
		"compile": "webpack",
		"compile-tests": "tsc -p . --outDir out",
		"package": "webpack --mode production --devtool hidden-source-map",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"vsce-package": "vsce package -o autopep8.vsix",
		"vscode:prepublish": "npm run package",
		"watch": "webpack --watch",
		"watch-tests": "tsc -p . -w --outDir out"
	},
	"contributes": {
		"commands": [
			{
				"category": "autopep8",
				"command": "autopep8.restart",
				"title": "%command.restartServer%"
			}
		],
		"configuration": {
			"properties": {
				"autopep8.args": {
					"default": [],
					"items": {
						"type": "string"
					},
					"markdownDescription": "%settings.args.description%",
					"scope": "resource",
					"type": "array"
				},
				"autopep8.cwd": {
					"default": "${workspaceFolder}",
					"examples": [
						"${workspaceFolder}/src",
						"${fileDirname}"
					],
					"markdownDescription": "%settings.cwd.description%",
					"scope": "resource",
					"type": "string"
				},
				"autopep8.importStrategy": {
					"default": "useBundled",
					"enum": [
						"useBundled",
						"fromEnvironment"
					],
					"markdownDescription": "%settings.importStrategy.description%",
					"markdownEnumDescriptions": [
						"%settings.importStrategy.useBundled.description%",
						"%settings.importStrategy.fromEnvironment.description%"
					],
					"scope": "resource",
					"type": "string"
				},
				"autopep8.interpreter": {
					"default": [],
					"items": {
						"type": "string"
					},
					"markdownDescription": "%settings.interpreter.description%",
					"scope": "resource",
					"type": "array"
				},
				"autopep8.path": {
					"default": [],
					"examples": [
						[
							"~/global_env/autopep8"
						],
						[
							"~/.env/python",
							"-m",
							"autopep8"
						]
					],
					"items": {
						"type": "string"
					},
					"markdownDescription": "%settings.path.description%",
					"scope": "resource",
					"type": "array"
				},
				"autopep8.showNotifications": {
					"default": "off",
					"enum": [
						"off",
						"onError",
						"onWarning",
						"always"
					],
					"markdownDescription": "%settings.showNotifications.description%",
					"markdownEnumDescriptions": [
						"%settings.showNotifications.off.description%",
						"%settings.showNotifications.onError.description%",
						"%settings.showNotifications.onWarning.description%",
						"%settings.showNotifications.always.description%"
					],
					"scope": "machine",
					"type": "string"
				}
			}
		},
		"languages": [
			{
				"filenames": [
					"pyproject.toml"
				],
				"id": "toml"
			},
			{
				"filenames": [
					"setup.cfg",
					"tox.ini",
					".pep8",
					".flake8"
				],
				"id": "ini"
			}
		]
	},
	"activationEvents": [
		"onLanguage:python",
		"workspaceContains:pyproject.toml",
		"workspaceContains:setup.cfg",
		"workspaceContains:tox.ini",
		"workspaceContains:.pep8",
		"workspaceContains:.flake8"
	],
	"dependencies": {
		"fs-extra": "^11.2.0",
		"vscode-languageclient": "^8.1.0"
	},
	"devDependencies": {
		"@types/fs-extra": "^11.0.4",
		"@types/glob": "^8.1.0",
		"@types/node": "16.x",
		"@types/vscode": "^1.74.0",
		"@vscode/test-electron": "^2.3.9",
		"@vscode/vsce": "^2.23.0",
		"glob": "^10.3.10",
		"ts-loader": "^9.5.1",
		"typescript": "^5.3.3",
		"typescript-esbuild": "0.4.5",
		"webpack": "^5.90.3",
		"webpack-cli": "^5.1.4"
	},
	"extensionDependencies": [
		"ms-python.python"
	],
	"publishConfig": {
		"access": "public"
	},
	"icon": "icon.png",
	"galleryBanner": {
		"color": "#1e415e",
		"theme": "dark"
	},
	"preview": true,
	"capabilities": {
		"virtualWorkspaces": {
			"description": "Virtual Workspaces are not supported with Autopep8 formatter.",
			"supported": false
		}
	},
	"serverInfo": {
		"module": "autopep8",
		"name": "autopep8"
	}
}
